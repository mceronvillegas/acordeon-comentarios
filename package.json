{
    "name": "react-start",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "eslint src/ --ext .ts,.tsx,.jsx --report-unused-disable-directives",
        "lint:fix": "eslint src/ --ext .ts,.tsx,.jsx --report-unused-disable-directives --fix",
        "preview": "npm run build && vite preview",
        "prepare": "husky",
        "format": "prettier --write .",
        "format:check": "prettier --check ."
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^7.3.2",
        "@mui/material": "^7.3.2",
        "@reduxjs/toolkit": "^2.9.1",
        "axios": "^1.12.2",
        "dayjs": "^1.11.18",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-redux": "^9.2.0",
        "react-router": "^7.9.3",
        "redux-persist": "^6.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.33.0",
        "@tailwindcss/vite": "^4.1.13",
        "@types/axios": "^0.9.36",
        "@types/react": "^19.1.10",
        "@types/react-dom": "^19.1.7",
        "@types/redux-persist": "^4.0.0",
        "@vitejs/plugin-react": "^5.0.0",
        "daisyui": "^5.1.25",
        "eslint": "^9.33.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^16.3.0",
        "husky": "^9.1.7",
        "prettier": "3.6.2",
        "tailwindcss": "^4.1.13",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.39.1",
        "vite": "^7.1.2"
    }
